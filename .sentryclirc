# .sentryclirc

# Do not store real tokens in source control. The CI should provide SENTRY_AUTH_TOKEN.

[defaults]
url = https://sentry.io/
org = ${SENTRY_ORG}
project = ${SENTRY_PROJECT}

[auth]

# The Sentry Webpack Plugin and CLI read SENTRY_AUTH_TOKEN from env in CI.

# In local development, export SENTRY_AUTH_TOKEN in your shell if you need uploads.

# token = ${SENTRY_AUTH_TOKEN}
